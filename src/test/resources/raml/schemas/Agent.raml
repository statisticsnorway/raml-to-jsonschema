#%RAML 1.0 Library
uses:
  IdentifiableArtefact: ../basetypes/IdentifiableArtefact.raml

types:

  Agent:
    description: Agent - an organization, induvidual or system.
    type: IdentifiableArtefact.IdentifiableArtefact
    properties:
      agentType:
        type: string
        description: The type of agent.
        displayName: Type aktør
        enum:
          - INDIVIDUAL
          - ORGANIZATION
          - SYSTEM
      parentAgent?:  #TODO: Foreløpig 0..1, men det er mulig vi må utvide denne til 0..* (mulighet til å peke på flere parents)
        type: string
        description: A reference to the parent object (id)
        displayName: Referanse til moder-objekt

    example: !include ../examples/AgentExample1.json
